            md_bg_color: '#4169E1'
            text_size: 20
            size_hint: (1,0.5)
            bold: True
            background_color: '#4169E1'
            theme_text_color: "Custom"
            text_color: "white"
            line_color: "green"
            icon_color: "white"
            on_press:
                app.root.current = "home"
            font_size: 30
            bold: True
            background_color: (1,1,1,.9)
            halign: 'center'
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos
            pos_hint: 0.9, 0.1
            size_hint: 0.2, 0.2
        MDLabel:
            text: "Instructions: Kindly Be Stationary and face towards the Camera"
            pos_hint: 0.9, 0.1
            size_hint: 0.1, 0.1
            halign: 'center'
            font_size: 10
            bold: True
            italic: True
        Image:
            id: video
            size_hint: 1, 1
            halign: 'center'
            allow_stretch: True

<OperationsScreen>:
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    name: "OP"
    MDNavigationLayout:
        ScreenManager:
            Screen:
                BoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: 'OPERATIONS'
                        md_bg_color: "#587eae"
                        elevation:4.5
                    Widget:
                    IncrediblyCrudeClock:
                        pos_hint: {"center_x": .5, "center_y": .7}
                        font_size: 20
                        bold: True
                        background_color: (1,1,1,1)
                        halign: 'center'
                        size_hint: 0.9, 0.2
                    MDBottomNavigation:
                        id: bottom_navigation
						text_color_active: 0, 0, 0, 1
						MDBottomNavigationItem:
						    id: home_tab
							name: 'HOME'
							text: 'HOME'
							icon: 'home'
	                        MDRectangleFlatButton:
	                            icon: "home-import-outline"
								text: 'Go to Home Page'
								pos_hint: {'center_x':0.5,'center_y':0.3}
								text_color: "white"
								line_color: "black"
								md_bg_color: "#89ade0"
								font_style : 'H4'
                                on_release:
                                    app.root.current = "home"
						MDBottomNavigationItem:
                            icon: "clock-check-outline"
                            text: " Clock IN "
	                        MDRectangleFlatButton:
	                            icon: "clock-plus-outline"
								text: 'ENTER'
								pos_hint: {'center_x':0.5,'center_y':0.3}
								text_color: "white"
								line_color: "black"
								md_bg_color: "#89ade0"
								icon_right: "clock-in"
								font_style : 'H4'
                                on_release: root.show_dialog_box_clock_in()
                        MDBottomNavigationItem:
							name: 'CLOCK-OUT'
							text: " Clock Out "
							icon: "clock-fast"
							MDRectangleFlatIconButton:
							    icon: "clock-fast"
                                text: 'CLOCK-OUT'
								pos_hint: {'center_x':0.5,'center_y':0.3}
								text_color: "white"
								line_color: "black"
								md_bg_color: "#89ade0"
								icon_right: "clock-in"
								font_style : 'H4'
                                on_release: root.show_dialog_box_clock_out()
                        MDBottomNavigationItem:
							name: 'ET'
							text: 'ENTER TIME'
                            icon: "timetable"
							MDRectangleFlatIconButton:
							    icon: "timetable"
								text: 'Click here to log your Job details'
								pos_hint: {'center_x':0.5,'center_y':0.3}
								text_color: "white"
								line_color: "black"
								md_bg_color: "#89ade0"
								icon_right: "clock-in"
								font_style : 'H4'
                                on_release:
                                    root.db_prompt()
                                    ## insert_employee_EnterTime(EID, EN)
                                    app.root.current = "ET"
                MDFloatLayout:
                    Image:
                        source : 'MMI.png'
                        size_hint: 0.4, 0.4
                        pos_hint: {"center_x": .5, "center_y": .7}

#:import Factory kivy.factory.Factory


#: import SpinnerOption kivy.uix.spinner.SpinnerOption
#: import dp kivy.metrics.dp

<MySpinnerOption@SpinnerOption>:
    background_color: "#FFFFFF"
    background_down: "#FFFFFF"
    ## font_size: dp(5)
    font_size: 5

<MySpinner@Spinner>:
    option_cls: MySpinnerOption
    background_color: "#FFFFFF"
    background_down: "#FFFFFF"
    ## font_size: dp(5)
    font_size: '5sp'








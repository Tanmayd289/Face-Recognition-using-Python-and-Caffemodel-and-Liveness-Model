<LoginScreen>:
    name: "FR"
    canvas.before:
        Color:
            rgba: (1,1,1,1)
        Rectangle:
            pos: self.pos
            size: self.size
    GridLayout:
        cols:1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}

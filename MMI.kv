        cols:1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        IncrediblyCrudeClock:
            font_size: 30
            bold: True
            background_color: (1,1,1,.9)
            halign: 'center'
            canvas.before:
                Color:
                    rgba: self.background_color
